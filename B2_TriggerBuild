node {
    try {
        dir("${env.WORKSPACE}/../${env.JOB_NAME}@script/"){
            stage('Build P0') {
                sh "python Main.py Build P0"
            }
            stage('Deploy P0') {
                sh "python Main.py Deploy P0"
            }
        }
    } catch (err) {
        currentBuild.result = 'FAILED'
        throw err
    }
}
